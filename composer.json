{
    "name": "ollywarren/makepackage",
    "description": "Implements a Artisan command to scaffold out laravel composer packages",
    "keywords": ["artisan","commands", "composer", "package"],
    "homepage": "https://github.com/ollywarren/artisan-make-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Olly Warren",
            "email": "olly@ollywarren.com"
        }
    ],
    "require-dev":{},
    "require":{
        "sebastiaanluca/php-stub-generator": "^0.1.1"
    },
    "autoload": {
        "psr-4": {
            "Ollywarren\\Makepackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ollywarren\\Makepackage\\MakepackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
